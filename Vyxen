Toggle = Page3:CreateToggle("Auto Rate toggler", "Loop activates 'Rating Bomber' and 'Custom Rating'", function(state)
    if state == true then
        bombAndCustom = true
        _G.bombCustom = true
    else
        bombAndCustom = false
        _G.bombCustom = false
    end
